   "source": [
    "import pandas as pd\n",
    "\n",
    "current_year = pd.Timestamp.now().year\n",
    "leap_years = [year for year in range(1, current_year) \n",
    "              if (year % 4 == 0 and year % 100 != 0) or (year % 400 == 0)]\n",
    "len(leap_years)"
   ]
  }
